{
  "manifest_version": 2,
  "name": "BlockX",
  "description": "Block access to time-consuming websites (chosen by you) to boost your productivity.",
  "version": "1.1",
  "options_page": "options.html",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false,
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
     }
  }
}